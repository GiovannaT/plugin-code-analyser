{
  "name": "cc-code-analyser",
  "displayName": "Cyclomatic Complexity Analyser",
  "description": "calculadora de complexidade para js/ts",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.103.2"
  },
  "publisher": "giovannat",
  "repository": {
    "type": "git",
    "url": "https://github.com/GiovannaT/plugin-code-analyser.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-analyser.analyzeProject",
        "title": "Complexity Analyzer: Analisar Projeto JS/TS"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vscode:prepublish": "pnpm run bundle",
    "bundle": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --minify",
    "package": "pnpm vsce package --no-dependencies",
    "publish": "pnpm vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "1.103.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.48.1"
  }
}
